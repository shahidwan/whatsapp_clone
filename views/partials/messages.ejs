<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>

<% if (error && error.length > 0) { %>
<script>
  Swal.fire({
    toast: true,
    position: "top-end",
    icon: "error",
    title: "<%= error[0] %>",
    showConfirmButton: false,
    timer: 3000,
    timerProgressBar: true,
  });
</script>
<% } %> <% if (success && success.length > 0) { %>
<script>
  Swal.fire({
    toast: true,
    position: "top-end",
    icon: "success",
    title: "<%= success[0] %>",
    showConfirmButton: false,
    timer: 3000,
    timerProgressBar: true,
  });
</script>
<% } %>
